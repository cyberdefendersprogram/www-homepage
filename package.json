{
  "name": "cyberdefenders",
  "version": "1.0.0",
  "description": "Bulma and others",
  "main": "_assets/css/mybulma.scss",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "sass assets/sass/main.scss:assets/css/mybulma.css",
    "css-watch": "npm run css-build -- --watch",
    "deploy": "npm run css-deploy && npm run js-build",
    "js-build": "babel assets/babeljs --out-dir assets/js",
    "js-watch": "npm run js-build -- --watch",
    "serve": "bundle exec jekyll serve --livereload",
    "start": "npm-run-all --parallel css-watch js-watch serve",
    "data": "cd _data/yaml-datagen && poetry run python yaml_datagen.py --gsheet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyberdefenders/homepage.git"
  },
  "keywords": [
    "sass"
  ],
  "author": "Vaibhav Bhandari",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyberdefenders/homepage/issues"
  },
  "homepage": "https://github.com/cyberdefenders/homepage#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-ie": "^6.7.0",
    "bulma": "^0.8.2",
    "npm-run-all": "^4.1.5"
  }
}
